# Frontend production environment variables example
# Copy this file to .env.production and edit with your actual values

# API URL for the backend server
NEXT_PUBLIC_API_URL=https://yourproductiondomain.com/api

# Authentication URL (if different from API URL)
NEXT_PUBLIC_AUTH_URL=https://yourproductiondomain.com/api

# Application URL
NEXT_PUBLIC_APP_URL=https://yourproductiondomain.com